{
  "name": "@freud-ds/icons",
  "version": "2.2.0",
  "description": "Zenklub - Freud DS Icons",
  "author": "Zenklub",
  "homepage": "",
  "license": "ISC",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist",
    "emoji",
    "fonts",
    "icon",
    "style",
    "iconList",
    "index.js",
    "index.d.ts",
    "index.ts"
  ],
  "scripts": {
    "build": "npm run clean && npm run generate && npm run copy:src && npm run copy:out && npm run copy:package",
    "clean": "rm -rf dist",
    "generate": "node generateIconList.mjs && tsc",
    "copy:src": "cp -r src ./dist",
    "copy:out": "cp -r out/. ./dist",
    "copy:package": "cp package.json dist/package.json"
  },
  "dependencies": {
    "fs-extra": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^16.18.59",
    "typescript": "^4.9.5"
  }
}
