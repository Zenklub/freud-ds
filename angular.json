{
  "version": 1,
  "projects": {
    "core-web": {
      "projectType": "application",
      "root": "apps/core-web",
      "sourceRoot": "apps/core-web/src",
      "prefix": "freud",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/core-web",
            "index": "apps/core-web/src/index.html",
            "main": "apps/core-web/src/main.ts",
            "polyfills": "apps/core-web/src/polyfills.ts",
            "tsConfig": "apps/core-web/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/core-web/src/favicon.ico",
              "apps/core-web/src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/@freud-ds/core-support/dist/assets",
                "output": "./assets/@freud-ds/"
              }
            ],
            "styles": [
              "apps/core-web/src/styles.scss",
              "node_modules/@freud-ds/core-support/dist/style/css/variables.css",
              "node_modules/@freud-ds/core-support/dist/style/scss/_variables.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/core-web/src/environments/environment.ts",
                  "with": "apps/core-web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "core-web:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "core-web:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "core-web:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/core-web/src/**/*.ts",
              "apps/core-web/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/core-web"],
          "options": {
            "jestConfig": "apps/core-web/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "apps/core-web/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/core-web",
            "config": {
              "configFolder": "apps/core-web/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "core-web-e2e": {
      "root": "apps/core-web-e2e",
      "sourceRoot": "apps/core-web-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/core-web-e2e/cypress.json",
            "tsConfig": "apps/core-web-e2e/tsconfig.e2e.json",
            "devServerTarget": "core-web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "core-web:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/core-web-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  }
}
